#Sorting a user defined list without using sort function
def fxn(s):
    lt=[]
    for i in range(s):
        y=int(input("Enter Elements: "))
        lt.append(y)
    for i in range(s):
        for j in range(s-i-1):
            if lt[j]>lt[j+1]:
                temp=lt[j]
                lt[j]=lt[j+1]
                lt[j+1]=temp
    print("List is: ",lt)

l=int(input("Enter Amount: "))
fxn(l)
